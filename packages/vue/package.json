{
  "name": "@ratoufa/iconsax-vue",
  "version": "1.0.1",
  "description": "940+ beautiful Iconsax icons for Vue 3 with full TypeScript support",
  "author": "Chris",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./icons/*": {
      "import": {
        "types": "./dist/icons/*.d.ts",
        "default": "./dist/icons/*.js"
      },
      "require": {
        "types": "./dist/icons/*.d.cts",
        "default": "./dist/icons/*.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/test-utils": "^2.4.3",
    "happy-dom": "^12.10.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "unplugin-vue": "^7.1.0",
    "vitest": "^1.1.0",
    "vue": "^3.4.0",
    "vue-tsc": "^3.2.2"
  },
  "keywords": [
    "vue",
    "vue3",
    "icons",
    "iconsax",
    "svg",
    "components",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ratoufa/iconsax-vue-next",
    "directory": "packages/vue"
  }
}
