{
  "name": "@ratoufa/iconsax-nuxt",
  "version": "1.0.1",
  "description": "Nuxt 3 module for Iconsax icons with auto-imports",
  "author": "Chris",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.0",
    "@ratoufa/iconsax-vue": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^4.3.0",
    "nuxt": "^3.9.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "icons",
    "iconsax",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ratoufa/iconsax-vue-next",
    "directory": "packages/nuxt"
  }
}
